<body class="align">

    <div class="grid">

        <form action="/{{roomId}}/joinMeeting/call" method="POST" class="form login">

            <div class="form__field">

                <label class="text-center">
                    Meeting Id : {{roomId}}
                </label>
            </div>

            <div class="form__field">

                <input id="login__password" type="password" name="password" class="form__input" placeholder="Password"
                    required>
            </div>

            <div class="form__field">
                <input type="submit" value="Join">
            </div>

        </form>

        <p class="text--center">Back to <a href="/">Home</a> <svg class="icon">
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="assets/images/icons.svg#arrow-right"></use>
            </svg></p>

    </div>



</body>